<div id="tareas"  v-cloak>
  <div id="search-tareas" class="input-group md-form form-sm form-1 pl-0">
    <div class="input-group-prepend">
      <span class="input-group-text cyan lighten-2" id="basic-text1"><i class="fas fa-search text-white"
          aria-hidden="true"></i></span>
    </div>
    <input class="form-control my-0 py-1" type="text" placeholder="Search" aria-label="Search">
  </div>
  <a class="btn-primary" href="/tareas-new">Crear Tareas</a>
  {{text}}<br>

  <% for ( i=0; i < resp.length; i++) { %>
    <div class="conten">
    <div class="row">
     <div class="col-2">
      <%= resp[i].nameTarea %>
     </div>
     <div class="col-4">
      <%= resp[i].descricion %>
     </div>
     <div class="col-4">
      <%= resp[i].prioridad %>
     </div>
    </div>
   </div>
 
  <% } %>
     
    

</div>